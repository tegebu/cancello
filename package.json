{
  "name": "@tegebu/cancello",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:tegebu/cancello.git"
  },
  "license": "MIT",
  "author": "tegebu <info@tegebu.com>",
  "sideEffects": false,
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "develop": "functions-framework --target=hello",
    "format": "eslint --fix 'src/**/*.{ts,tsx}'",
    "start": "node dist/index.js",
    "test": "jest test --detectOpenHandles --coverage",
    "update": "yarn update:upgrade && yarn update:install",
    "update:install": "yarn install && yarn upgrade",
    "update:upgrade": "ncu --upgrade"
  },
  "dependencies": {},
  "devDependencies": {
    "@google-cloud/functions-framework": "1.7.1",
    "@types/express": "4.17.7",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.2",
    "@types/sinon": "9.0.5",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "@typescript-eslint/typescript-estree": "3.10.1",
    "eslint": "7.7.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "lint-staged": "10.2.13",
    "npm-check-updates": "7.1.1",
    "rimraf": "3.0.2",
    "sinon": "9.0.3",
    "sort-package-json": "1.44.0",
    "supertest": "4.0.2",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": "npm is not supported"
  },
  "engineStrict": true
}
